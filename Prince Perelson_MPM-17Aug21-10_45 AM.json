  "createPerm": {
    "entity": "JobSubmission",
    "filter": {
      "status": [
        "Placed"
      ],
      "jobOrder.employmentType": "Contract",
      "candidate.employeeType": [
        "1099"
      ]
    },
    "updates": {
      "candidate": {
        "status": "Placed"
      },
      "jobOrder": {
        "isOpen": false
      },
      "jobSubmission": {
        "status": "Placed"
      }
